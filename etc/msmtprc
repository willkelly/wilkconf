defaults
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile ~/wilkconf/logs/msmtp.log

account gmail
host smtp.gmail.com
from the.william.kelly@gmail.com
auth on
usereval emacsclient -a "" --eval '(offlineimap-get-user "smtp.gmail.com" "465")' | tr -d '"'
passwordeval emacsclient -a "" --eval '(offlineimap-get-password "smtp.gmail.com" "465")' | tr -d '"'

account work
host rackermail.cannot.name
from william.kelly@rackspace.com
auth on
usereval emacsclient -a "" --eval '(offlineimap-get-user "rackermail.cannot.name" "465")' | tr -d '"'
passwordeval emacsclient -a "" --eval '(offlineimap-get-password "rackermail.cannot.name" "465")' | tr -d '"'

# Set a default account
account default : gmail